define simple_move_if_change_rule
  if [ -s $@-t ]; then \
    $(top_srcdir)/build-aux/move-if-change $@-t $@; \
  else \
    echo "$@-t is empty!" 1>&2; \
    rm -f $@-t; \
    exit 1; \
  fi
endef

define cp_update_rule
  if [ "x$(srcdir)" != "x." ] && [ -f $(srcdir)/$@ ] && [ ! -f $@ ]; then \
    cp $(srcdir)/$@ $@; \
    touch -r $(srcdir)/$@ $@; \
  fi
endef

