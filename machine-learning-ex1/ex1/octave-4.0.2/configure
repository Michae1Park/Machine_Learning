### Check version number when using gcc.

GCC_VERSION=
if test "$GCC" = yes; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking C compiler version number" >&5
$as_echo_n "checking C compiler version number... " >&6; }
  gcc_version=`$CC -v 2>&1 | $GREP "^.*gcc version" | \
    $SED -e 's/^.*g.. version *//' -e 's/cygnus-//' -e 's/egcs-//' -e 's/ .*//'`




  # Used to indicate true or false condition
  ax_compare_version=false

  # Convert the two version strings to be compared into a format that
  # allows a simple string comparison.  The end result is that a version
  # string of the form 1.12.5-r617 will be converted to the form
  # 0001001200050617.  In other words, each number is zero padded to four
  # digits, and non digits are removed.

  ax_compare_version_A=`echo "$gcc_version" | sed -e 's/\([0-9]*\)/Z\1Z/g' \
                     -e 's/Z\([0-9]\)Z/Z0\1Z/g' \
                     -e 's/Z\([0-9][0-9]\)Z/Z0\1Z/g' \
                     -e 's/Z\([0-9][0-9][0-9]\)Z/Z0\1Z/g' \
                     -e 's/[^0-9]//g'`


  ax_compare_version_B=`echo "3" | sed -e 's/\([0-9]*\)/Z\1Z/g' \
                     -e 's/Z\([0-9]\)Z/Z0\1Z/g' \
                     -e 's/Z\([0-9][0-9]\)Z/Z0\1Z/g' \
                     -e 's/Z\([0-9][0-9][0-9]\)Z/Z0\1Z/g' \
                     -e 's/[^0-9]//g'`


    ax_compare_version=`echo "x$ax_compare_version_A
x$ax_compare_version_B" | sed 's/^ *//' | sort -r | sed "s/x${ax_compare_version_A}/false/;s/x${ax_compare_version_B}/true/;1q"`



    if test "$ax_compare_version" = "true" ; then
    warn_gcc_version="gcc version $gcc_version is likely to cause problems"

  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: $warn_gcc_version" >&5
$as_echo "$as_me: WARNING: $warn_gcc_version" >&2;}


      fi


  GCC_VERSION=$gcc_version
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $GCC_VERSION" >&5
$as_echo "$GCC_VERSION" >&6; }
fi


