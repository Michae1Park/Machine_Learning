### More configure argument checking related to linking

# Check whether --enable-no-undefined was given.
if test "${enable_no_undefined+set}" = set; then :
  enableval=$enable_no_undefined; case $enableval in
     yes) NO_UNDEFINED_LDFLAG="-no-undefined" ;;
     no)  NO_UNDEFINED_LDFLAG="" ;;
     *) as_fn_error $? "bad value $enableval for --disable-no-undefined" "$LINENO" 5 ;;
   esac
else
  NO_UNDEFINED_LDFLAG="-no-undefined"
fi



# Check whether --enable-link-all-dependencies was given.
if test "${enable_link_all_dependencies+set}" = set; then :
  enableval=$enable_link_all_dependencies; case $enableval in
     yes) link_all_deps=yes ;;
     no)  link_all_deps=no ;;
     *) as_fn_error $? "bad value $enableval for --enable-link-all-depenencies" "$LINENO" 5
     ;;
   esac
else
  link_all_deps=no
fi

